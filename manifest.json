{
  "manifest_version": 3,
  "name": "Tab Monitor",
  "description": "CPU, memory, and bandwidth usage monitor for Chrome tabs.",
  "permissions": ["processes", "tabs"],
  "action": {
    "title": "Tab Monitor"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "version": "0.1.0",
  "icons": {
    "16": "images/chrome-eye-16.png",
    "32": "images/chrome-eye-32.png",
    "48": "images/chrome-eye-48.png",
    "128": "images/chrome-eye-128.png"
  }
}
